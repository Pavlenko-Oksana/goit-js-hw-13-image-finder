<ul class="gallery">
    {{#each this}}
    <li class="gallery-item">
        <div class="photo-card">
            <img src="{{webformatURL}}" alt="{{tags}}" data-sourse="{{largeImageURL}}" />
            {{!-- <div class="modal">
                <img src="{{largeImageURL}}" width="800" height="600">
            </div> --}}
            <div class="stats">
                <p class="stats-item">
                    <i class="material-icons">thumb_up</i>
                    {{likes}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">visibility</i>
                    {{views}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">comment</i>
                    {{comments}}
                </p>
                <p class="stats-item">
                    <i class="material-icons">cloud_download</i>
                    {{downloads}}
                </p>
            </div>
        </div>
    </li>
    {{/each}}
</ul>